  "devDependencies": {
    "jest": "27.5.1",
    "standard": "14.3.1"
  },
  "standard": {
    "env": [
      "jest"
    ]
  },
  "jest": {
    "testMatch": [
      "**/test/**/*test.[jt]s?(x)",
